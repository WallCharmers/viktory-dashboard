# Streamlit Secrets Configuration Example
# Copy this file to .streamlit/secrets.toml and fill in your actual values

# App Authentication
app_password = "changeme"

# Amazon SP-API Credentials (Optional - for live data)
[sp_api]
client_id = "your_sp_api_client_id"
client_secret = "your_sp_api_client_secret" 
refresh_token = "your_sp_api_refresh_token"

# AWS Configuration (Optional - for SP-API role assumption)
[aws]
access_key_id = "your_aws_access_key_id"
secret_access_key = "your_aws_secret_access_key"
region = "us-east-1"
role_arn = "arn:aws:iam::your_account:role/your_role_name"

# WallCharmers Configuration (Optional)
[wallcharmers]
seller_id = "your_seller_id"
marketplace_id = "ATVPDKIKX0DER"